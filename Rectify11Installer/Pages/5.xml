<duixml>
	<Element resid="PageMain" layoutpos="client" layout="borderlayout()" background="argb(0,0,0,0)">
		<Element id="atom(animator5)" class="animator" layoutpos="left"/>
		<Element layout="borderlayout()" class="page" padding="rect(-50rp,0,0,0)" layoutpos="left">
			<Element layout="flowlayout(0,2,2,0)" padding="rect(60rp,10rp,60rp,0)" layoutpos="left">
				<Element class="CMPImage" width="250rp" height="250rp"/>
			</Element>
			<Element layout="borderlayout()" layoutpos="left">
				<Richtext class="headertext" layoutpos="top" content="Select Components to install"/>
				<Richtext class="instructiontext" layoutpos="top" content="You can select what Rectify11 components should be installed on your PC."/>
				<Element id="atom(CmpContainer)" layout="verticalflowlayout(0,2,0,0)" padding="rect(10rp,15rp,0,0)" layoutpos="left">
					<TouchCheckBox id="atom(chk1)" checkedstate="checked" content="Themes"/>
					<TouchCheckBox id="atom(chk2)" checkedstate="checked" content="Accent Colorizer"/>
					<TouchCheckBox id="atom(chk3)" checkedstate="checked" content="Enhanced Winver and shutdown dialog"/>
					<TouchCheckBox id="atom(chk4)" checkedstate="checked" content="Enhanced Explorer layout"/>
				</Element>
			</Element>
		</Element>
	</Element>
</duixml>